input_line = input().split(" ")
stu = int(input_line[0])
ok = int(input_line[1])

index = 1
for i in range(stu):
    data = input().split(" ")
    point = int(data[0])
    off = int(data[1])
    total_point = point - off * 5
    if total_point < 0:
        total_point = 0
    if total_point >= ok:
        print(index)
        index += 1
    else:
        index +=1
