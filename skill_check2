line1 = input().split(" ")
a = int(line1[0])
b = int(line1[1])
r = int(line1[2])
num = int(input())

for i in range(num):
    xy = input().split(" ")
    x = int(xy[0])
    y = int(xy[1])
    position = (x - a) ^2 + (y - b) ^2
    R2 = r ^2
    
    if position < R2:
        print("noisy")
    else:
        print("silent")
